<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="280dp"
    android:layout_height="280dp"
    tools:context="clothesup.turningpoint.clothesup.PopupActivity"
    android:orientation="vertical"
    android:background="@drawable/square_line">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="B 31"
            android:id="@+id/textview_popup_store_id"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="25dp"
            android:textColor="#000000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="|"
            android:id="@+id/textView2"
            android:textColor="#777777"
            android:layout_alignTop="@+id/textview_popup_store_id"
            android:layout_toRightOf="@+id/textview_popup_store_id"
            android:layout_toEndOf="@+id/textview_popup_store_id"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="꼬까옷"
            android:id="@+id/textview_popup_store_name"
            android:textColor="#000000"
            android:layout_alignTop="@+id/textView2"
            android:layout_toRightOf="@+id/textView2"
            android:layout_toEndOf="@+id/textView2" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="15dp"
            android:id="@+id/imageView"
            android:layout_below="@+id/textview_popup_store_id"
            android:layout_alignLeft="@+id/textview_popup_store_id"
            android:layout_alignStart="@+id/textview_popup_store_id"
            android:src="@drawable/kind_"
            android:layout_marginTop="5dp" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="14dp"
            android:id="@+id/imageView5"
            android:src="@drawable/style_"
            android:layout_alignTop="@+id/imageView"
            android:layout_toRightOf="@+id/imageView"
            android:layout_toEndOf="@+id/imageView" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="14dp"
            android:id="@+id/imageView6"
            android:src="@drawable/style_"
            android:layout_alignTop="@+id/imageView5"
            android:layout_toRightOf="@+id/imageView5"
            android:layout_toEndOf="@+id/imageView5"
            android:layout_marginLeft="2dp" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="14dp"
            android:id="@+id/imageView7"
            android:src="@drawable/style_"
            android:layout_alignTop="@+id/imageView6"
            android:layout_toRightOf="@+id/imageView6"
            android:layout_toEndOf="@+id/imageView6"
            android:layout_marginLeft="2dp" />

        <ImageView
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:id="@+id/imageView8"
            android:src="@drawable/favorite"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@+id/imageView9"
            android:layout_toStartOf="@+id/imageView9"
            android:layout_marginTop="15dp" />

        <ImageView
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:id="@+id/imageView9"
            android:src="@drawable/history"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="15dp"
            android:layout_marginRight="13dp" />
    </RelativeLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="1dp"></LinearLayout>
</LinearLayout>