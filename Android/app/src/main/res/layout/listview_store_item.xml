<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="60dp"
    android:padding="10dp">

    <TextView
        android:id="@+id/textView_storeRank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:text="1"
        android:textSize="15sp" />

    <ImageView
        android:id="@+id/imageView_storeImage"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/textView_storeRank"
        android:layout_marginStart="15dp"
        android:layout_toEndOf="@+id/textView_storeRank"
        android:layout_toRightOf="@+id/textView_storeRank"
        android:src="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/textView_storeNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView_storeRank"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_toEndOf="@+id/imageView_storeImage"
        android:layout_toRightOf="@+id/imageView_storeImage"
        android:text="B 301"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView_storeName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView_storeNum"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_toEndOf="@+id/textView_storeNum"
        android:layout_toRightOf="@+id/textView_storeNum"
        android:text="꼬까옷"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/textView_storeHit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView_storeName"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        android:text="조회수"
        android:textSize="15dp" />

</RelativeLayout>